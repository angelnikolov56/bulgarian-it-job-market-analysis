knitr::opts_chunk$set(echo = TRUE)
library(tidyverse)
library(ggthemes)
theme_set(theme_tufte(base_size = 14))
jobs <- read_rds("../data/dev.bg-jobs-clean.rds")
glimpse(jobs)
jobs %>%
count(company_name, sort = TRUE) %>%
slice_head(n = 20) %>%
ggplot(aes(x = reorder(company_name, n), y = n)) +
geom_col(fill = "steelblue") +
geom_text(aes(label = n),
position = position_stack(vjust = 1),
color = "white",
size = 3.1,
hjust = 1.5) +
coord_flip() +
labs(y = "Count",
x = "Company Name",
title = "Top 20 Companies by Number of Job Offers")
jobs %>%
count(work_model, sort = TRUE) %>%
ggplot(aes(x = reorder(work_model, n), y = n)) +
geom_col(fill = "steelblue") +
geom_text(aes(label = n),
position = position_stack(vjust = 1),
color = "white",
size = 5.5,
hjust = 1.5) +
coord_flip() +
labs(y = "Count",
x = "Work Model",
title = "Number of Jobs by Work Model")
jobs %>%
count(location) %>%
ggplot(aes(x = reorder(location, n), y = n)) +
geom_col(fill = "steelblue") +
geom_text(aes(label = n),
vjust = 0.5,
color = "black",
size = 3.5,
hjust = -0.3) +
coord_flip() +
labs(y = "Count",
x = "Location",
title = "Number of Jobs by Location")
jobs %>%
count(seniority) %>%
ggplot(aes(x = reorder(seniority, n), y = n)) +
geom_col(fill = "steelblue") +
geom_text(aes(label = n),
position = position_stack(vjust = 1),
color = "white",
size = 5.5,
hjust = 1.5) +
coord_flip() +
labs(y = "Count",
x = "Seniority",
title = "Number of Jobs by Seniority")
top_20_technologies <- jobs %>%
unnest(tech_stack) %>%
filter(tech_stack != "English") %>%
count(tech_stack, sort = TRUE) %>%
head(20)
top_20_technologies$tech_stack <- factor(top_20_technologies$tech_stack, levels = rev(top_20_technologies$tech_stack))
top_20_technologies %>%
ggplot(aes(x = tech_stack, y = n)) +
geom_col(fill = "steelblue", width = 0.8) +
geom_text(aes(label = n),
position = position_stack(vjust = 1),
color = "white",
size = 3.5,
hjust = 1.5) +
coord_flip() +
labs(x = "Technology",
y = "Number of Jobs",
title = "Top 20 Technologies Required in Job Listings")
category_broad <- c("Backend Development", "Frontend Development", "Fullstack Development", "Infrastructure", "Quality Assurance", "PM/BA and more", "Mobile Development",
"Data Science", "ERP / CRM Development", "Hardware and Engineering", "Customer Support", "UI/UX, Arts", "Technical Support")
avg_tech_count_by_category <- jobs %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
mutate(tech_count = map_int(tech_stack, length)) %>%
group_by(categories) %>%
summarize(tech_count = mean(tech_count)) %>%
arrange(-tech_count)
avg_tech_count_by_category$categories <- factor(avg_tech_count_by_category$categories, levels = rev(avg_tech_count_by_category$categories))
avg_tech_count_by_category %>%
ggplot(aes(x = categories, y = tech_count)) +
geom_col(fill = "steelblue", width = 0.8) +
geom_text(aes(label = round(tech_count, 2)),
position = position_stack(vjust = 1),
color = "white",
size = 3.5,
hjust = 1.5) +
coord_flip() +
labs(x = "Broad Category",
y = "Average Number of Technologies",
title = "Average Number of Technologies Required by Category")
jobs %>%
mutate(tech_count = map_int(tech_stack, length)) %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
group_by(job_link, categories) %>%
summarize(tech_count = first(tech_count)) %>%
ggplot(aes(x = categories, y = tech_count, fill = categories)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Broad Category",
y = "Number of Technologies Required",
title = "Distribution of the Number of Technologies Required by Category")
jobs %>%
mutate(tech_count = map_int(tech_stack, length)) %>%
ggplot(aes(x = seniority, y = tech_count, fill = seniority)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Seniority",
y = "Number of Technologies Required",
title = "Distribution of the Number of Technologies Required by Seniority")
broad_categories <- jobs %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
count(categories, sort = TRUE)
broad_categories$categories <- factor(broad_categories$categories, levels = rev(broad_categories$categories))
broad_categories %>%
ggplot(aes(x = categories, y = n)) +
geom_col(fill = "steelblue", width = 0.8) +
geom_text(aes(label = n),
position = position_stack(vjust = 1),
color = "white",
size = 3.5,
hjust = 1.5) +
coord_flip() +
labs(x = "Broad Category",
y = "Number of Jobs",
title = "Number of Jobs by Broad Category")
jobs_salary <- jobs %>%
filter(!is.na(salary_min) & !is.na(salary_max))
jobs_salary %>%
summarize(avg_min_salary = mean(salary_min),
avg_max_salary = mean(salary_max),
median_min_salary = median(salary_min),
median_max_salary = median(salary_max),
sd_min_salary = sd(salary_min),
sd_max_salary = sd(salary_max))
jobs_salary %>%
ggplot(aes(x = salary_min)) +
geom_histogram(binwidth = 500,
fill = "grey",
color = "steelblue") +
labs(x = "Salary Lower Range",
y = "Number of Jobs",
title = "Distribution of Salaries' Lower Range")
jobs_salary %>%
ggplot(aes(x = salary_max)) +
geom_histogram(binwidth = 1000,
fill = "grey",
color = "steelblue") +
labs(x = "Salary Upper Range",
y = "Number of Jobs",
title = "Distribution of Salaries' Upper Range")
jobs_salary %>%
ggplot(aes(x = seniority, y = salary_min, fill = seniority)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Seniority",
y = "Lower Salary Range",
title = "Distribution of Lower Range Salaries by Seniority Level")
jobs_salary %>%
ggplot(aes(x = seniority, y = salary_max, fill = seniority)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Seniority",
y = "Upper Salary Range",
title = "Distribution of Upper Range Salaries by Seniority Level")
jobs_salary %>%
ggplot(aes(x = seniority, y = salary_min, fill = seniority)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Seniority",
y = "Lower Salary Range",
title = "Distribution of Lower Range Salaries by Seniority Level")
jobs_salary %>%
ggplot(aes(x = seniority, y = salary_max, fill = seniority)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Seniority",
y = "Upper Salary Range",
title = "Distribution of Upper Range Salaries by Seniority Level")
jobs_salary %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
group_by(categories) %>%
summarize(avg_min_salary = mean(salary_min),
avg_max_salary = mean(salary_max),
median_min_salary = median(salary_min),
median_max_salary = median(salary_max),
min_min_salary = min(salary_min),
max_max_salary = max(salary_max),
sd_min_salary = sd(salary_min),
sd_max_salary = sd(salary_max))
jobs_salary %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
ggplot(aes(x = categories, y = salary_min, fill = categories)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Category",
y = "Lower Salary Range",
title = "Distribution of Lower Range Salaries by Category")
jobs_salary %>%
unnest(categories) %>%
filter(categories %in% category_broad) %>%
ggplot(aes(x = categories, y = salary_max, fill = categories)) +
geom_boxplot(show.legend = FALSE) +
coord_flip() +
labs(x = "Category",
y = "Upper Salary Range",
title = "Distribution of Upper Range Salaries by Category")
jobs_salary %>%
mutate(tech_count = map_int(tech_stack, length)) %>%
group_by(tech_count) %>%
summarize(median_min_salary = median(salary_min),
median_max_salary = median(salary_max)) %>%
arrange(-median_max_salary)
job_salary_by_tech <- jobs_salary %>%
unnest(tech_stack) %>%
filter(tech_stack != "English") %>%
group_by(tech_stack) %>%
summarize(median_min_salary = median(salary_min),
median_max_salary = median(salary_max)) %>%
arrange(-median_max_salary) %>%
slice_head(n = 20)
job_salary_by_tech$tech_stack <- factor(job_salary_by_tech$tech_stack, levels = rev(job_salary_by_tech$tech_stack))
job_salary_by_tech %>%
ggplot(aes(x = reorder(tech_stack, median_max_salary), y = median_max_salary)) +
geom_col(fill = "steelblue", width = 0.8) +
coord_flip() +
labs(x = "Technology",
y = "Median Upper Salary Range",
title = "Top 20 Median Upper Salary Ranges by Technology",
subtitle = "All jobs and technologies included")
job_salary_by_tech_adj <- jobs_salary %>%
unnest(tech_stack) %>%
filter(tech_stack != "English") %>%
group_by(tech_stack) %>%
summarize(median_min_salary = median(salary_min),
median_max_salary = median(salary_max),
n_jobs = n()) %>%
filter(n_jobs >= 10) %>%
arrange(-median_max_salary) %>%
slice_head(n = 20)
job_salary_by_tech_adj$tech_stack <- factor(job_salary_by_tech_adj$tech_stack, levels = rev(job_salary_by_tech_adj$tech_stack))
job_salary_by_tech_adj %>%
ggplot(aes(x = reorder(tech_stack, median_max_salary), y = median_max_salary)) +
geom_col(fill = "steelblue", width = 0.8) +
coord_flip() +
labs(x = "Technology",
y = "Median Upper Salary Range",
title = "Highest Median Upper Salary Ranges by Technology",
subtitle = "For technologies listed in at least 10 job offers")
